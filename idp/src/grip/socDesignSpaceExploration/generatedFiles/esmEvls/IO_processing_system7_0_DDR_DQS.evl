context EObject{
	
	constraint size {		
		check : self->size() = 1 
	}
	
	constraint type {
		guard: self.satisfies("size")		
		check : ExternalPortReferenceType.all.size() = 1		
	}

}

context ExternalPortReferenceType{

	constraint name {
		check: self.portRef = "processing_system7_0_DDR_DQS"
	}
}